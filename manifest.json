{
  "manifest_version": 2,
  "name": "Note Annotate",
  "version": "1.0",
  "description": "Note annotate",
  "icons": {
    "48": "/icon/note icon.ico"
  },
  // "chrome_url_overrides": { //Only for testing
  //   "newtab": "main.html?home=true"
  // },
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "48": "/icon/note icon.ico",
      "64": "/icon/note icon.ico"
    },
    "default_title": "Note Annotate",
    "default_popup": "main.html"
  },
  "applications": {
    "gecko": {
      "id": "himkeat@gmail.com", //Used for storage
      "strict_min_version": "1.0"
    }
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ]
}